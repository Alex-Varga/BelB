}

.custom-select {
  position: relative;
  z-index: 11;
  font-size: 14px;
  font-weight: bold;

  &.custom-select--open {
	.custom-select__default {
	  svg {
		transform: translateY(-50%) rotateZ(90deg);
	  }
	}

	.custom-select__list {
	  display: block;
	}
  }

  .custom-select__default {
	padding-top: 17px;
	padding-bottom: 17px;
	padding-left: 0;
	padding-right: 25px;
	background-color: $white;
	border-bottom: 2px solid #E4E4E4;
	text-transform: uppercase;
	color: $navy;
	cursor: pointer;

	svg {
	  position: absolute;
	  top: 50%;
	  right: 3px;
	  width: 9px;
	  transform: translateY(-50%) rotateZ(-90deg);
	  transition-property: transform;
	  transition-duration: 0.2s;

	  path {
		fill: $pink;
	  }
	}
  }

