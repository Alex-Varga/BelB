.product-card {
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;

  &.product-card--featured {
	.product-card__title {
	  margin-bottom: auto;
	}

	.product-card__button {
	  max-width: 172px;
	  margin-left: auto;
	  margin-right: auto;
	}
  }

  .product-card__images-wrapper {
	position: relative;
	width: 100%;
	max-width: 200px;
	height: 200px;
	margin: 0 auto;

	@include hover {
	  .product-card__image-wrapper {
		&.product-card__image-wrapper--first {
		  opacity: 0;
		  visibility: hidden;
		}

		&.product-card__image-wrapper--second {
		  opacity: 1;
		  visibility: visible;
		}
	  }
	}

	&.product-card__images-wrapper--no-hover {
	  @include hover {
		.product-card__image-wrapper {
		  &.product-card__image-wrapper--first {
			opacity: 1;
			visibility: visible;
		  }

		  &.product-card__image-wrapper--second {
			opacity: 0;
			visibility: hidden;
		  }
		}
	  }
	}
  }

  .product-card__image-wrapper {
	transition-property: opacity, visibility;
	transition-duration: 0.3s;

	&.product-card__image-wrapper--second {
	  opacity: 0;
	  visibility: hidden;
	}
  }

  .product-card__image {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	object-fit: contain;
	object-position: 50% 50%;
  }

  img {
	box-shadow: none;
  }

  .product-card-options {
	position: static;
	visibility: visible;
	opacity: 1;
	width: 100%;
	max-width: 200px;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 15px;
	padding: 0;
	transform: translateY(0);
	box-shadow: none;
  }

  .product-card__badge {
	position: absolute;
	top: 0;
	right: 0;
	display: block;
	max-width: 80px;
	transform: translate(50%, -50%);
  }

  .product-card__badge-image {
	position: relative;
	z-index: 9;
	display: block;
	width: 100%;
	height: 100%;
	will-change: filter;
  }

  .product-card__title {
	display: block;
	margin-top: 23px;
	margin-bottom: 15px;
	padding-left: 10px;
	padding-right: 10px;
	font-size: 16px;
	font-weight: 700;
	text-transform: uppercase;
	text-decoration: none;
	text-align: center;
	line-height: 1;
	color: inherit;
  }

  .product-card__short-description,
  .product-card__hint,
  .product-card__swatcher {
	max-width: 200px;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 15px;
  }

  .product-card__short-description {
	text-align: left;
	font-size: 18px;
	color: $navy;
  }

  .product-card__hint {
	font-size: 14px;
	color: #000;
  }

  .product-card__price {
	margin-top: 20px;
	margin-bottom: 20px;
	font-size: 24px;
	font-weight: 700;
	text-transform: uppercase;
	text-align: center;
	line-height: 1;
  }

  .product-card__button {
	width: 100%;
	max-width: 220px;
	min-width: 0;
  }

  .product-card__button {
	display: flex;
	align-items: center;
	justify-content: center;
	max-width: 203px;
	margin-left: auto;
	margin-right: auto;
	padding-top: 15px;
	padding-bottom: 15px;
	font-size: 16px;

	&.product-card__button--success {
	  svg.icon-customize,
	  svg.icon-cart {
		visibility: hidden;
		opacity: 0;
		transform: translate(20px, -50%);
	  }

	  svg.icon-check {
		visibility: visible;
		opacity: 1;
		transform: translate(0, calc(-50% + 2px));
	  }
	}

	&[disabled] {
	  background-color: $grey-disabled;
	  border-color: $grey-disabled;
	  pointer-events: none;
	}

	@include hover {
	  svg {
		path {
		  fill: $pink;
		}
	  }
	}
  }

  .product-card__button-icon {
	position: relative;
	display: block;
	width: 20px;
	margin-right: 20px;

	svg {
	  position: absolute;
	  top: 50%;
	  left: 0;
	  width: 20px;
	  transform: translateY(-50%);
	  transition-property: opacity, visibility, transform;
	  transition-duration: 0.3s;

	  path {
		fill: #fff;
		transition: fill 0.3s;
	  }
	}

	svg.icon-check {
	  visibility: hidden;
	  opacity: 0;
	  transform: translate(-20px, calc(-50% + 2px));
	}
  }

  .product-card__actions-wrapper {
	max-width: 191px;
	margin-right: auto;
	margin-bottom: 5px;
	margin-left: auto;
  }

  .product-card__hr {
	width: 100%;
	max-width: 200px;
	height: 2px;
	margin-top: auto;
	margin-left: auto;
	margin-right: auto;
	background-color: #CBCBCB;
  }

  @include breakpoint(small) {
