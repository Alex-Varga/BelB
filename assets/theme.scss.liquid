/*================ UI ================*/
.button {
  display: inline-block;
  padding: 18px 20px;
  min-width: 191px;

  font-family: $font-family;
  font-size: 20px;
  font-weight: bold;
  text-align: center;
  text-transform: uppercase;
  text-decoration: none;
  line-height: 1;

  border-radius: 30px;
  box-shadow: none;
  border: 2px solid transparent;

  transition-property: background-color, color, border-color;
  transition-duration: 0.4s;


  &.button--primary {
	color: #fff;
	border-color: $blue;
	background-color: $blue;

	@include hover {
	  color: $blue;
	  border-color: $blue;
	  background-color: #fff;
	}
  }

  &.button--primary-reverse {
	color: $blue;
	border-color: $blue;
	background-color: #fff;

	@include hover {
	  color: #fff;
	  border-color: $blue;
	  background-color: $blue;
	}
  }

  &.button--secondary {
	color: #fff;
	border-color: $pink;
	background-color:$pink;

	@include hover {
	  color: $pink;
	  // border-color: #fff;
	  background-color: #fff;
	}
  }

  &.button--secondary-reverse {
	color: $pink;
	border-color: $pink;
	background-color: #fff;

	@include hover {
	  color: #fff;
	  background-color: $pink;
	}
  }

  &.button--transparent {
	color: $blue;
	border-color: $blue;
	background-color: transparent;

	@include hover {
	  color: #fff;
	  background-color: $blue;
	}
  }

  &.button--small {
	min-width: 172px;
  }

  &.button--medium {
	min-width: 220px;
  }

  &.button--large {
	min-width: 322px;
  }

  // Disabled
  &.disabled,
  &[disabled] {

	&:hover,
	&:focus {

	}
  }
}

