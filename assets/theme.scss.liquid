// Header
.header {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 13;
  display: block;
  width: 100%;
  padding-top: 20px;
  padding-bottom: 20px;
  background-color: var(--bg-color);
  transition-property: padding;
  transition-duration: 0.3s;

  .header-mobile-menu {
	padding-top: 100px;
  }

  &.header--scrolled {
	padding-top: 10px;
	padding-bottom: 10px;

	.header__logo-image {
	  max-height: 50px;
	}

	.header-mobile-menu {
	  padding-top: 60px;
	}
  }

  .header__container {
	display: flex;
	align-items: center;
	justify-content: space-between;
	max-width: 1440px;
	margin: 0 auto;
	padding-left: 40px;
	padding-right: 40px;
  }

  .header__logo {
	flex-basis: 25%;
  }

  .header__logo-image {
	display: inline-block;
	max-height: 60px;
	vertical-align: middle;
	transition-property: max-height;
	transition-duration: 0.3s;
  }

  .header__main-menu {
	flex-grow: 1;
  }

  .header__secondary-menu {
	flex-basis: 25%;
	display: flex;
	align-items: flex-end;
	justify-content: flex-end;
	margin-left: -15px;
	margin-right: -15px;
  }

  .header__secondary-menu-item {
	display: inline-flex;
	margin-left: 15px;
	margin-right: 15px;
  }

  .header__secondary-menu-link {
	display: inline-flex;
	align-items: center;
	transition-property: opacity;
	transition-duration: 0.3s;

	.icon {
	  display: block;
	  width: unset;
	  height: 30px;
	}

	&:hover {
	  opacity: 0.75;
	}
  }

  .header__secondary-menu-cart,
  .header__mobile-cart {
	position: relative;
	margin-right: 10px;
  }

  .header__secondary-menu-cart {
	.icon {
	  height: 26px;
	}
  }

  .header__secondary-menu-cart-count,
  .header__mobile-cart-count {
	position: absolute;
	top: -10px;
	right: -10px;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	width: 19px;
	height: 19px;
	border-radius: 50%;
	background-color: $navy;
	font-size: 12px;
	font-weight: 700;
	color: $white;
  }

  .header__menu-button {
	position: relative;
	z-index: 90;
	display: none;
	flex-direction: column;
	justify-content: center;
	width: 30px;
	height: 30px;
	cursor: pointer;

	span {
	  display: block;
	  width: 100%;
	  height: 2px;
	  margin-top: 3px;
	  border-radius: 2px;
	  background-color: $navy;
	  transform-origin: 50% 50%;
	  transition-property: background-color, transform;
	  transition-duration: 0.3s;

	  &:nth-child(1) {
		margin-top: 0;
	  }

	  &:nth-child(2) {
		width: 15px;
	  }

	  &:nth-child(3) {
		width: 20px;
	  }
	}

	&.header__menu-button--open {
	  span {
		margin-top: 0;
		background-color: $pink;

		&:nth-child(1) {
		  width: 100%;
		  transform: rotate(44deg) translateX(3px);
		}

		&:nth-child(2) {
		  transform: rotate(45deg) scale(0);
		}

		&:nth-child(3) {
		  width: 100%;
		  transform: rotate(-45deg) translateX(3px);
		}
	  }
	}
  }

  .header__mobile-cart {
	z-index: 101;
	display: none;

	.icon {
	  display: block;
	  width: unset;
	  height: 30px;
	}
  }

  @include breakpoint(medium) {
	padding-top: 5px;
	padding-bottom: 5px;

	.header-mobile-menu {
	  padding-top: 60px;
	}

	&.header--scrolled {
	  padding-top: 5px;
	  padding-bottom: 5px;
	}

	.header__container {
	  padding-left: 20px;
	  padding-right: 20px;
	}

	.header__logo {
	  flex-basis: unset;
	}

	.header__logo-image {
	  max-height: 50px;
	}

	.header__menu-button {
	  display: flex;
	}

	.header__secondary-menu {
	  display: none;
	}

	.header__mobile-cart {
	  display: block;
	}
  }
}

.header-main-menu {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: -25px;
  margin-right: -25px;

  .header-main-menu__item {
	margin-left: 25px;
	margin-right: 25px;

	&:hover {
	  .header-main-menu__link {
		color: $pink;
	  }

	  .header-main-menu__link-chevron {
		transform: rotate(-180deg) translate3d(0, 0, 0);
	  }

	  .header-main-menu__dropdown {
		visibility: visible;
		opacity: 1;
		transform: translateY(0);
	  }
	}
  }

  .header-main-menu__link {
	position: relative;
	display: flex;
	align-items: center;
	font-family: $font-family;
	font-size: 18px;
	font-weight: 700;
	text-align: center;
	text-transform: uppercase;
	text-decoration: none;
	color: $navy;

	transition-property: color;
	transition-duration: 0.3s;

	&:before {
	  content: '';
	  position: absolute;
	  top: 100%;
	  left: 0;
	  width: 100%;
	  height: 40px;
	  cursor: auto;
	}
  }

  .header-main-menu__link-chevron {
	display: inline-block;
	margin-left: 7px;

	transition-property: transform;
	transition-duration: 0.3s;

	.icon {
	  display: block;
	  max-width: 11px;

	  svg path {
		fill: $navy;
	  }
	}
  }

  .header-main-menu__dropdown {
	position: absolute;
	top: 100%;
	visibility: hidden;
	opacity: 0;
	transform: translateY(-20px);

	transition-property: visibility, opacity, transform;
	transition-duration: 0.3s, 0.3s, 0.2s;
	transition-timing-function: ease-in;
  }

  .header-main-menu__dropdown-link {
	display: block;
	padding-top: 14px;
	padding-bottom: 14px;
	padding-left: 10px;
	padding-right: 14px;

	background-color: $white;
	font-size: 12px;
	font-weight: 700;
	line-height: 1;
	text-transform: uppercase;
	text-decoration: none;
	color: $navy;

	transition-property: background-color, color;
	transition-duration: 0.3s;

	&:hover {
	  background-color: $light-pink;
	  color: $pink;
	}
  }

  @include breakpoint(medium) {
	display: none;
  }
}

.header-mobile-menu {
  display: block;
  position: fixed;
  z-index: 100;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;

  transform: translateX(-100%);
  transition-property: transform, padding-top;
  transition-duration: 0.6s;

  &.header-mobile-menu--open {
	transform: translateX(0);
  }

  .header-mobile-menu__wrapper {
	overflow-y: auto;
	-webkit-overflow-scrolling: touch;

	max-width: 375px;
	height: 100%;
	padding-top: 20px;
	padding-bottom: 100px;
	padding-left: 60px;
	padding-right: 60px;
	background-color: $blue;
  }

  .header-mobile-menu__item {
	+ .header-mobile-menu__item {
	  margin-top: 36px;
	}
  }

  .header-mobile-menu__link {
	font-size: 16px;
  }

  .header-mobile-menu__link,
  .header-mobile-menu__dropdown-link {
	font-weight: 700;
	text-transform: uppercase;
	text-decoration: none;
	color: $white;
  }

  .header-mobile-menu__dropdown {
	padding-left: 18px;
  }

  .header-mobile-menu__dropdown-item {
	margin: 20px 0;
  }

  .header-mobile-menu__dropdown-link {
	display: inline-block;
	font-size: 24px;
  }

  .header-mobile-menu__secondary {
	display: flex;
	justify-content: center;
	margin-top: 25px;
  }

  .header-mobile-menu__secondary-menu-link {
	&:not(:last-of-type) {
	  margin-right: 35px;
	}

	svg {
	  width: 100%;
	  max-width: 30px;
	  max-height: 30px;
	}
  }
}


// Promobar
.promobar {
  padding-top: 16px;
  padding-bottom: 16px;
  background-color: var(--bg-color);

  .promobar__container {
	padding-left: 20px;
	padding-right: 20px;
  }

  .promobar__text {
	overflow: hidden;
	display: block;
	width: 100%;
	font-size: 18px;
	font-weight: 700;
	text-align: center;
	text-transform: uppercase;
	text-decoration: none;
	text-overflow: ellipsis;
	color: var(--text-color);
	white-space: nowrap;
  }

  @include breakpoint(small-tablet-portrait) {
	padding-top: 6px;
	padding-bottom: 6px;
  }
}


// PDP
.product-section {
  padding-top: 28px;
  padding-bottom: 50px;

  .product-section__container {
	max-width: 1242px;
	margin-left: auto;
	margin-right: auto;
	padding-left: 20px;
	padding-right: 20px;
  }

  .product-section__row {
	display: flex;
  }

  .product-section__col {
	flex: 0 0 50%;
	max-width: 50%;

	&.product-section__col--media {
	  padding-left: 51px;
	  padding-right: 50px;
	}

	&.product-section__col--content {
	  padding-left: 20px;
	}
  }

  .product-section__thumbnails-wrapper {
	position: relative;
	margin-top: 20px;

	&.product-section__thumbnails-wrapper--static {
	  .product-section__thumbnails {
		display: flex;
		margin-left: -5px;
		margin-right: -5px;
	  }

	  .product-section__thumbnails-arrow {
		display: none;
	  }

	  .product-section__thumbnail {
		min-width: 20%;
		transition: opacity 0.3s;

		&.product-section__thumbnail--active {
		  opacity: 0.3;
		}
	  }
	}
  }

  .product-section__slider-wrapper {
	position: sticky;
	top: 90px;
  }

  .product-section__thumbnails {
	.slick-list {
	  margin-left: -5px;
	  margin-right: -5px;
	}

	.slick-slide {
	  transition: opacity 0.3s;
	}

	.slick-current {
	  opacity: 0.3;
	}
  }

  .product-section__thumbnails-arrow {
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	cursor: pointer;

	svg path {
	  fill: $pink;
	  transition: fill 0.3s;
	}

	@include hover {
	  svg path {
		fill: $navy;
	  }
	}

	&.product-section__thumbnails-arrow--left {
	  left: -30px;
	}

	&.product-section__thumbnails-arrow--right {
	  right: -30px;


	  svg {
		transform: rotate(180deg);
	  }
	}
  }

  .product-section__thumbnails-item {
	position: relative;
	overflow: hidden;
	display: block;
	margin-left: 5px;
	margin-right: 5px;
	padding-top: 100%;
	cursor: pointer;
  }

  .product-section__thumbnails-image {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	object-fit: contain;
	object-position: 50% 50%;
  }

  .product-section__content {
	color: $navy;
  }

  .product-section__title {
	margin-bottom: 20px;
	font-size: 48px;
	font-weight: 700;
	text-transform: uppercase;
  }

  .product-section__rating {
	.yotpo.bottomLine {
	  display: inline-block;
	}

	.yotpo-bottomline {
	  display: flex;
	  align-items: flex-end;
	}

	.yotpo-icon {
	  font-size: 22px !important;
	  line-height: 1.3;
	  color: #FF9900 !important;
	}

	.text-m {
	  margin-left: 14px;
	  font-family: $font-family;
	  font-size: 18px;
	  font-weight: 700;
	  line-height: 1.3;
	  color: $pink !important;
	}
  }

  .product-section__description {
	margin-top: 20px;
	font-size: 18px;
	line-height: 1.3;
  }

  .product-section__form {
	margin-top: 40px;
  }

  .product-section__label {
	display: inline-block;
	margin-right: 12px;
	font-size: 18px;
	font-weight: 700;
	line-height: 1.3;

	&.product-section__label--after-price {
	  margin-left: 12px;
	  margin-right: 0;
	}
  }

  .product-section__line {
	display: flex;
	justify-content: space-between;
	align-items: flex-end;
	flex-wrap: wrap;

	&.product-section__line--left {
	  justify-content: flex-start;
	  align-items: center;
	  margin-top: 26px;
	}

	&.product-section__line--mobile {
	  display: none;
	}
  }

  .product-section__additional-information {
	display: flex;
	align-items: center;
	width: calc(100% - 215px);
	margin-left: 15px;
	padding-left: 15px;
	border-left: 3px solid #C4C4C4;
	font-size: 18px;
  }

  .product-section__variants-item {
	font-size: 16px;
	text-transform: uppercase;

	+ .product-section__variants-item {
	  margin-top: 20px;
	}
  }

  .product-section__input-radio {
	label {
	  padding-left: 40px;
	}
  }

  .product-section__variant-size {
	font-size: 24px;
	font-weight: 700;
  }

  .product-section__variant-price {
	font-size: 24px;
	font-weight: 700;

	&.product-section__variant-price--old {
	  font-size: 18px;
	  text-decoration: line-through;
	  color: rgba($navy, 0.5);
	}
  }

  .product-section__price {
	margin-top: 26px;
	padding-right: 20px;
  }

  .product-section__price-value {
	font-size: 50px;
	font-weight: 700;
	line-height: 1;
	color: $pink;

	&.product-section__price-value--old {
	  font-size: 40px;
	  text-decoration: line-through;
	  color: rgba(#000, 0.3);
	}
  }

  .product-section__quantity {
	display: flex;
	align-items: center;
  }

  .product-section__button-wrapper {
	width: 100%;
	max-width: 304px;
	margin-top: 40px;

	&.product-section__button-wrapper--mobile {
	  display: none;
	}
  }

  .product-section__button {
	width: 100%;
	padding: 14px 30px;
	border-radius: 11px;

	svg {
	  width: 30px;
	  margin-right: 12px;
	  vertical-align: text-bottom;

	  path {
		fill: $white;
		transition: fill 0.4s;
	  }
	}

	@include hover {
	  svg {
		path {
		  fill: $pink;
		}
	  }
	}

	&[disabled] {
	  background-color: $grey-disabled;
	  border-color: $grey-disabled;
	  pointer-events: none;
	}
  }

  .product-section__ingredients-popup-button-wrapper {
	margin-top: 20px;
  }

  .product-section__ingredients-popup-button {
	font-size: 18px;
	font-weight: bold;
	line-height: 1;
	text-transform: uppercase;
	text-decoration: underline;
	color: $pink;
	transition: opacity 0.4s;

	@include hover {
	  opacity: 0.7;
	}
  }

  .product-section__mobile {
	display: none;
  }

  .product-section__desktop-labels {
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	margin: 0 -5px;
  }

  .product-section__variant-label-wrapper {
	padding: 0 5px;
	width: 50%;
	min-width: fit-content;
	margin-bottom: 10px;
  }

  .product-section__variant-radio-button {
	display: none;
  }

  .product-swatcher {
	justify-content: center;
  }

  .product-section__variant-label {
	position: relative;
	display: inline-block;
	width: 100%;
	padding-top: 8px;
	padding-bottom: 9px;
	padding-left: 20px;
	padding-right: 50px;
	font-size: 20px;
	line-height: 1.3;
	border-radius: 11px;
	background-color: $white;
	border: 3px solid #E4E4E4;
	cursor: pointer;
	transition: 0.4s border-color;

	&.checked {
	  border-color: $pink;

	  .product-section__variant-label-button {
		svg {
		  path[d="M8 13.5l5.5 7 7-11"] {
			stroke: $pink;

			transition: 0.4s stroke;
		  }

		  ellipse {
			fill: #FFCE9F;

			transition: 0.4s fill;
		  }
		}
	  }
	}

	@include hover {
	  border-color: $pink;
	}
  }

  .product-section__variant-label-button {
	position: absolute;
	top: 50%;
	right: 15px;
	width: 29px;
	height: 29px;
	transform: translateY(-50%);

	svg {
	  width: 29px;

	  path[d="M8 13.5l5.5 7 7-11"] {
		fill: none;
		stroke: none;

		transition: 0.4s stroke;
	  }

	  ellipse {
		fill: #fff;

		transition: 0.4s fill;
	  }
	}
  }

  &.product-section--giftcard {
	.product-section__variant-size {
	  display: none;
	}

	@include breakpoint(small-tablet-portrait) {
	  .product-section__variant-price {
		display: block;
	  }

	  .product-section__variant-label-wrapper {
		flex-grow: 1;
		max-width: 100%;
	  }
	}
  }

  .product-section__giftship-features {
	width: 100%;
  }

  @include breakpoint(small) {
	.product-section__row {
	  flex-direction: column;
	}

	.product-section__col {
	  flex: 0 0 100%;
	  max-width: 100%;

	  &.product-section__col--content {
		padding-left: 0;
		padding-right: 0;
	  }
	}

	.product-section__content {
	  margin-top: 55px;
	}
  }

  @include breakpoint(extra-small) {
	.product-section__title {
	  font-size: 34px;
	}

	.product-section__description {
	  margin-top: 17px;
	}

	.product-section__button-wrapper {
	  margin-top: 26px;
	}
  }

  @include breakpoint(small-tablet-portrait) {
	padding-bottom: 40px;

	.product-section__col {
	  &.product-section__col--media {
		padding-left: 25px;
		padding-right: 25px;
	  }
	}

	.product-section__thumbnails-arrow {
	  svg {
		width: 12px;
	  }

	  &.product-section__thumbnails-arrow--left {
		left: -25px;
	  }

	  &.product-section__thumbnails-arrow--right {
		right: -25px;
	  }
	}

	.product-section__title {
	  font-size: 24px;
	}

	.product-section__ingredients-popup-button-wrapper {
	  text-align: center;
	}

	.product-section__form {
	  margin-top: 26px;
	}

	.product-section__button {
	  padding: 14px 15px;
	  width: fit-content;
	  min-width: fit-content;
	}

	.product-section__desktop-labels {
	  justify-content: center;
	}

	.product-section__variant-label-wrapper {
	  max-width: 145px;
	}

	.product-section__variant-label {
	  padding-left: 10px;
	  padding-right: 50px;
	}

	.product-section__variant-price {
	  display: none;
	}

	.product-section__line {
	  justify-content: center;

	  &.product-section__line--left {
		justify-content: space-between;
		margin-top: 0;
	  }

	  &.product-section__line--mobile {
		display: flex;
		justify-content: center;
	  }
	}

	.product-section__button-wrapper {
	  display: none;

	  &.product-section__button-wrapper--mobile {
		display: block;
		width: fit-content;
		min-width: fit-content;
		margin-top: 22px;
	  }
	}

	.product-section__additional-information {
	  width: 100%;
	  margin-top: 10px;
	  margin-left: 0;
	  padding-left: 0;
	  border-left: none;

	  &.product-section__additional-information {
		display: none;
	  }
	}

	.product-section__price {
	  margin-top: 19px;
	  padding-right: 0;

	  &.product-section__price--desktop {
		display: none;
	  }
	}

	.product-section__quantity {
	  align-items: flex-start;
	  flex-direction: column;
	}

	.product-section__customize-button {
	  width: 100%;
	  margin-top: 19px;

	  .product-section__button {
		width: 100%;
	  }
	}
  }

  @include breakpoint(mobile-landscape) {
	.product-swatcher {
	  .product-swatcher__option label {
		font-size: 24px;
	  }

	  .product-swatcher__swatch {
		label {
		  width: 100px;
		  height: 35px;

		  &.checked .product-swatcher__swatch-button {
			left: calc(100% - 56px);
		  }
		}

		.product-swatcher__swatch-button {
		  width: 56px;
		}

		input:checked+label .product-swatcher__swatch-button {
		  left: calc(100% - 56px);
		}
	  }
	}

	.product-section__line {
	  flex-direction: column;
	  align-items: center;

	  &.product-section__line--left {
		flex-direction: row;
	  }
	}

	.product-section__quantity {
	  flex-wrap: wrap;
	}

	.product-section__label {
	  width: 100%;

	  &.product-section__label--after-price {
		width: fit-content;
	  }
	}
  }

  @media (max-width: 360px) {
	.product-quantity-selector {
	  width: 130px;
	  margin-right: 10px;

	  .product-quantity-selector__button {
		width: 43px;
	  }
	}
  }

  @media (max-width: 340px) {
	.product-quantity-selector {
	  width: 120px;

	  .product-quantity-selector__button {
		width: 38px;
	  }
	}
  }
}

.ingredients {
  padding-top: 30px;
  padding-bottom: 20px;
  background-color: #ffebd8;

  .ingredients__container {
	width: 100%;
	max-width: 1315px;
	margin-left: auto;
	margin-right: auto;
	padding-left: 60px;
	padding-right: 60px;
  }

  .ingredients__wrapper {
	display: flex;
	justify-content: center;
	flex-wrap: wrap;
	margin: 0 -20px;
  }

  .ingredients__item {
	width: 16.66%;
	margin-bottom: 10px;
	padding: 0 20px;
	text-align: center;
  }

  .ingredients__image-wrapper {
	width: 72px;
	margin: 0 auto;
	object-fit: contain;
  }

  .ingredients__image {
	display: inline-block;
  }

  .ingredients__text {
	margin-top: 6px;
	font-size: 14px;
	font-weight: 600;
	line-height: 1;
  }

  @include breakpoint(extra-small) {
	.ingredients__container {
	  padding-left: 30px;
	  padding-right: 30px;
	}

	.ingredients__wrapper {
	  margin: 0 -30px;
	}

	.ingredients__item {
	  width: 33.333%;
	}
  }

  @include breakpoint(small-tablet-portrait) {
	padding-top: 50px;
	padding-bottom: 40px;

	.ingredients__container {
	  padding-left: 20px;
	  padding-right: 20px;
	}

	.ingredients__wrapper {
	  margin: 0 -15px;
	}

	.ingredients__image-wrapper {
	  width: 56px;
	}

	.ingredients__text {
	  margin-top: 3px;
	  font-size: 12px;
