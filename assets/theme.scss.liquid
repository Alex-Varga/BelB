  .discover-slider__image {
	max-width: 80px;

	img {
	  max-height: 110px;
	}

	&:not(:last-of-type) {
	  margin-right: 30px;
	}
  }

  .discover-slider__button-wrapper {
	flex-shrink: 0;

	.button {
	  min-width: 172px;
	}
  }

  @include breakpoint(medium) {
