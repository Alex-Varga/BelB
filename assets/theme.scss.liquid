// Article
.article {
  margin-bottom: 70px;

  .article__container {
	overflow: hidden;
	max-width: 945px;
	margin: 0 auto;
	padding-left: 20px;
	padding-right: 20px;

	//&.article__container--content {
	//	max-width: 945px;
	//}
  }

  .article__banner {
	position: relative;
	padding-top: 70px;
	padding-bottom: 115px;
  }

  .article__banner-image {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	object-fit: cover;
	object-position: 50% 50%;
  }

  .article__banner-content {
	position: relative;
	max-width: 600px;
	color: var(--text-color, #ffffff);
  }

  .article__headline {
	margin-bottom: 20px;
	font-size: 24px;
  }

  .article__headline,
  .article__title {
	text-align: left;
	color: var(--text-color, #ffffff);
  }

  .article__excerpt {
	margin-top: 20px;
	font-size: 18px;
	line-height: 1.4;
  }

  .article__content {
	padding-top: 50px;
  }

  .article__button {
	margin-top: 30px;
  }

  .article__slider-wrapper {
	position: relative;
	max-width: 1270px;
	margin-top: 70px;
	margin-bottom: 30px;
	margin-left: auto;
	margin-right: auto;
	padding-left: 80px;
	padding-right: 80px;

	&.article__slider-wrapper--static {
	  .article__slider {
		display: flex;
		flex-wrap: wrap;
		justify-content: center;
		margin-left: -10px;
		margin-right: -10px;
	  }

	  .article__slider-arrow {
		display: none;
	  }

	  .article__slider-item {
		width: calc(33.33% - 20px);
	  }
	}
  }

  .article__slider {
	.slick-list {
	  margin-left: -10px;
	  margin-right: -10px;
	}
  }

  .article__slider-arrow {
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	cursor: pointer;

	svg path {
	  fill: $pink;
	  transition: fill 0.3s;
	}

	@include hover {
	  svg path {
		fill: $navy;
	  }
	}

	&.article__slider-arrow--left {
	  left: 20px;
	}

	&.article__slider-arrow--right {
	  right: 20px;

	  svg {
		transform: rotate(180deg);
	  }
	}
  }

  .article__slider-item {
	display: block;
	margin-left: 10px;
	margin-right: 10px;
  }

  .article__slider-image-wrapper {
	position: relative;
	overflow: hidden;
	padding-top: 150%;
  }

  .article__slider-image {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	object-fit: cover;
	object-position: 50% 50%;
  }

  @include breakpoint(large) {
	.article__slider-wrapper {
	  &.article__slider-wrapper--static {
		padding-left: 20px;
		padding-right: 20px;
	  }
	}
  }

  @include breakpoint(medium) {
	.article__content {
	  padding-top: 30px;
	}

	.article__slider-wrapper {
	  padding: 40px 60px;
	}
  }

  @include breakpoint(small) {
	.article__banner {
	  padding-bottom: 70px;
	}

	.article__slider-wrapper {
	  &.article__slider-wrapper--static {
		.article__slider-item {
		  flex-grow: 1;
		  width: calc(50% - 20px);
		  margin-bottom: 20px;
		}
	  }
	}
  }

  @include breakpoint(mobile-landscape) {
	.article__button {
	  display: block;
	}
  }
}

