.input-radio {
  label {
	position: relative;
	display: block;
	padding-left: 40px;
	cursor: pointer;

	p {
	  display: inline-block;
	}

	&::before {
	  content: '';
	  position: absolute;
	  top: 50%;
	  left: 0;
	  width: 16px;
	  height: 16px;
	  border: 2px solid $dark;
	  border-radius: 50%;
	  transform: translateY(-50%);
	  transition-property: border-color;
	  transition-duration: 0.3s;
	}

	&::after {
	  content: '';
	  position: absolute;
	  top: 50%;
	  left: 5px;
	  width: 10px;
	  height: 10px;
	  background-color: $pink;
	  border-radius: 50%;
	  transform: translateY(-50%) scale(0);
	  transition-property: transform;
	  transition-duration: 0.3s;
	}
  }

  input[type=radio] {
	display: none;
  }

  input[type=radio]:checked {
	+ label {
	  &:before {
		border-color: $pink;
	  }

	  &:after {
		transform: translateY(-50%) scale(1);
	  }
	}
  }

  @media screen and (max-width: 1024px) {
	label {
	  padding-left: 26px;
	}
  }
}

