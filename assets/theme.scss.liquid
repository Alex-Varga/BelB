// Footer
.footer {
  padding-top: 92px;
  font-family: $font-family;

  .footer__container {
	max-width: 1200px;
	margin-left: auto;
	margin-right: auto;
	padding-left: 20px;
	padding-right: 20px;
  }

  .footer__row {
	display: flex;
	justify-content: space-between;

	&.footer__row--second {
	  margin-top: 30px;
	}
  }

  .footer__column {
	flex-basis: 33.3%;
  }

  .footer__link {
	text-decoration: none;
	color: inherit;
	transition-property: opacity;
	transition-duration: 0.3s;

	&:hover {
	  opacity: 0.75;
	}
  }

  .footer__newsletter-wrapper {
	flex-basis: 40%;
  }

  .footer__menu-wrapper {
	display: flex;
	justify-content: flex-end;
	flex-basis: 40%;
  }

  .footer-menu {
	min-width: 130px;

	&:not(:last-of-type) {
	  margin-right: 30px;
	}
  }

  .footer__logo {
	display: block;
	max-width: 141px;
	width: 100%;
	margin-left: auto;
	margin-right: auto;
  }

  .footer__socials-wrapper {
	display: flex;
	justify-content: flex-end;
	align-self: center;
  }

  .footer__bottom-info {
	margin-top: 5px;
	padding-top: 5px;
	padding-bottom: 10px;
	text-align: center;
	font-weight: bold;
	line-height: 1.4;
	color: $white;
  }

  .footer__copyright {
	font-size: 14px;
  }

  .footer__address {
	display: inline-block;
	margin-left: 3px;
	font-size: 16px;
  }

  @include breakpoint(medium) {
	.footer__newsletter-wrapper {
	  flex-basis: 43%;
	}

	.footer__menu-wrapper {
	  flex-basis: 50%;
	}
  }

  @include breakpoint(small) {
	padding-top: 96px;

	.footer__row {
	  &.footer__row--first {
		flex-direction: column;
		align-items: center;
	  }

	  &.footer__row--second {
		max-width: 640px;
		margin-top: 60px;
		margin-left: auto;
		margin-right: auto;
		flex-direction: column-reverse;
	  }
	}

	.footer__newsletter-wrapper {
	  flex-basis: 100%;
	  max-width: 461px;
	}

	.footer__socials-wrapper {
	  flex-basis: 40%;
	  align-self: center;
	}

	.footer__menu-wrapper {
	  flex-basis: 100%;
	  justify-content: center;
	  width: 100%;
	  max-width: 540px;
	  margin-top: 50px;
	}

	.footer__logo-wrapper {
	  margin-top: 35px;
	}

	.footer__copyright {
	  margin-top: 30px;
	}
  }

  @include breakpoint(mobile-landscape) {
	padding-top: 113px;

	.footer__row {
	  &.footer__row--second {
		margin-top: 74px;
	  }
	}

	.footer-menu {
	  min-width: auto;
	}

	.footer__menu-wrapper {
	  margin-top: 38px;
	}

	.footer__bottom-info {
	  margin-top: 20px;
	  padding-bottom: 15px;
	}

	.footer__address {
	  font-size: 14px;
	}
  }
}

.footer-menu {
  .footer-menu__heading {
	margin-bottom: 20px;
	text-transform: uppercase;
	font-size: 16px;
	font-weight: bold;
	color: $white;
  }

  .footer-menu__item {
	&:not(:last-of-type) {
	  margin-bottom: 18px;
	}
  }

  .footer-menu__link {
	text-transform: uppercase;
	text-decoration: none;
	font-size: 12px;
	font-weight: bold;
	color: $white;

	&:hover {
	  text-decoration: underline;
	}
  }
}

.footer-socials {
  .footer-socials__social {
	display: inline-block;
	width: 30px;
	height: 30px;
	margin-left: 8px;
	margin-right: 8px;
	transition-property: opacity;
	transition-duration: 0.3s;

	&:hover {
	  opacity: 0.75;
	}

	&:first-of-type {
	  .footer-socials__social-link {
		path {
		  fill: #fff;
		}
	  }
	}
  }

  .footer-socials__social-link {
	svg {
	  width: 30px;
	  height: 30px;
	}
  }
}

.newsletter {
  position: relative;
  width: 100%;
  padding: 32px 57px;
  padding-bottom: 26px;
  background-color: $beige;
  border-radius: 25px;

  &.newsletter--no-bg {
	padding-top: 0;
	padding-bottom: 0;
	background-color: inherit;
  }

  .newsletter__image {
	position: absolute;
	top: 0;
	left: 37px;
	transform: translateY(calc(-100% + 4px));

	img {
	  width: 100%;
	  max-width: 71px;
	}
  }

  .newsletter__info-wrapper {
	margin-top: 15px;
  }

  .newsletter__form-wrapper {
	display: flex;
  }

  .newsletter__form-field {
	width: 100%;
	border-right: none;
	background-color: $white;
	transition-property: background-color;
	transition-duration: 0.3s;

	&:focus,
	&:not(:placeholder-shown) {
	  background-color: #fff;
	}
  }

  .newsletter__form-button {
	min-width: auto;
	padding-top: 14px;
	padding-bottom: 12px;
	padding-right: 16px;
	padding-left: 16px;
	background-color: $navy;
	font-size: 14px;
	color: $white;

	border-radius: 0;

	&:hover {
	  background-color: $pink;
	}
  }

  .newsletter__form-messages {
	margin-top: 6px;
	font-size: 13px;
	color: $navy;
  }

  .newsletter__form-message {
	&.newsletter__form-message--success {
	  margin-top: 4px;
	  margin-bottom: 30px;
	  text-align: center;
	  font-size: 18px;
	  font-weight: bold;
	  line-height: 1.3;
	  color: $pink;
	}
  }

  .newsletter__form-error {
	color: red;
  }

  .newsletter__text {
	text-align: center;
	text-transform: uppercase;
	font-size: 14px;
	font-weight: bold;
	line-height: 1.3;
	color: $navy;

	&.newsletter__text--highlighted {
	  margin-top: 2px;
	  color: $pink;
	}

	a {
	  text-decoration: none;
	  color: inherit;
	  transition-property: opacity;
	  transition-duration: 0.3s;

	  &:hover {
		opacity: 0.75;
	  }
	}
  }

  @include breakpoint(mobile-landscape) {
	padding: 32px 20px;
	padding-bottom: 24px;
  }
}


// Home
.home-discover {
  padding-top: 80px;
  padding-bottom: 90px;
  background-color: var(--bg-color);
  font-family: $font-family;

  .home-discover__container {
	width: 100%;
	max-width: 1300px;
	margin-left: auto;
	margin-right: auto;
	padding-left: 15px;
	padding-right: 15px;
  }

  .home-discover__tabs {
	margin-top: 20px;
	max-width: 1100px;
	margin-left: auto;
	margin-right: auto;
  }

  .home-discover__slider-wrapper {
	max-width: 940px;
	margin-left: auto;
	margin-right: auto;
	margin-top: 46px;
  }

  @include breakpoint(medium) {
	.home-discover__slider-wrapper {
	  padding-left: 30px;
	  padding-right: 30px;
	}

	.home-discover__tabs {
	  max-width: 980px;
	  margin-top: 17px;
	}
  }

  @include breakpoint(small-tablet-portrait) {
	padding-top: 57px;
	padding-bottom: 60px;

	.home-discover__container {
	  padding-left: 5px;
	  padding-right: 5px;
	}

	.home-discover__slider-wrapper {
	  margin-top: 20px;
	  padding-left: 0;
	  padding-right: 0;
	}
  }
}

.card-tabs {
  display: flex;
  justify-content: center;

  .card-tabs__tab {
	display: flex;
	justify-content: center;
	align-items: center;
	flex-basis: 30%;
	margin-left: 10px;
	margin-right: 10px;
	padding: 10px 20px;
	background-color: var(--tabs-color);
	box-shadow: 6px 6px 10px #DFC3A7;
	border-radius: 20px;
	font-weight: bold;
	color: $navy;
	transition-property: background-color, color;
	transition-duration: 0.3s;
	cursor: pointer;

	&.card-tabs__tab--active {
	  background-color: var(--tabs-hover-color);
	  color: $white;

	  path {
		fill: $white;
	  }
	}

	path {
	  fill: $navy;
	  transition-property: fill;
	  transition-duration: 0.3s;
	}

	@include hover() {
	  background-color: var(--tabs-hover-color);
	  color: $white;

	  path {
		fill: $white;
	  }
	}
  }

  .card-tabs__icon {
	display: inline-flex;
	margin-right: 15px;
	width: 100%;
	max-width: 50px;
  }

  .card-tabs__title {
	text-transform: uppercase;
	font-size: 24px;
  }

  @include breakpoint(medium) {
	.card-tabs__icon {
	  margin-right: 10px;

	  svg {
		width: 100%;
		max-width: 40px;
		max-height: 40px;
	  }
	}

	.card-tabs__title {
	  font-size: 18px;
	}
  }

  @include breakpoint(small-tablet-portrait) {
	padding-left: 10px;
	padding-right: 10px;

	.card-tabs__tab {
	  flex-direction: column;
	  flex-basis: 33.3%;
	  margin-left: 0;
	  margin-right: 0;

	  &:not(:last-of-type) {
		margin-right: 5px;
	  }
	}

	.card-tabs__icon {
	  max-width: 25px;
	  margin-right: 0;
	  margin-bottom: 7px;

	  svg {
		max-width: 25px;
		max-height: 30px;
	  }
	}

	.card-tabs__title {
	  font-size: 14px;
	}
  }

  @include breakpoint(mobile-landscape) {
	.card-tabs__tab {
	  padding: 6px 10px;
	}

	.card-tabs__icon {
	  svg {
		max-width: 23px;
		max-height: 23px;
	  }
	}
  }
}

.discover-slider {
  position: relative;

  .discover-slider__arrow {
	position: absolute;
	top: 50%;
	z-index: 1;
	max-width: 23px;
	transform: translateY(-50%);
	cursor: pointer;

	&.discover-slider__arrow--left {
	  left: -105px;
	}

	&.discover-slider__arrow--right {
	  right: -105px;
	  transform: rotate(180deg) translateY(calc(50% + 4px));
	}

	&:hover {
	  path {
		fill: $pink;
	  }
	}

	svg {
	  width: 100%;
	}

	path {
	  transition-property: fill;
	  transition-duration: 0.3s;
	}
  }

  .discover-slider__slide {
	margin-left: 10px;
	margin-right: 10px;
  }

  .discover-slider__slide-container {
	display: flex;
	justify-content: space-between;
  }

  .discover-slider__slide-image-wrapper {
	flex-basis: 37.5%;

	img {
	  box-shadow: none;
	}
  }

  .discover-slider__slide-image {
	width: 100%;
	display: block;
  }

  .discover-slider__slide-content {
	flex-basis: 62.5%;
	padding-left: 80px;
	padding-top: 35px;
  }

  .discover-slider__slide-title {
	margin-bottom: 15px;
	text-align: left;
	font-size: 36px;
  }

  .discover-slider__slide-text {
